
i64 sqrt(i64 n) {
	// Implementation used from stackoverflow.com/a/1101217

	i64 op = n;
	i64 res = 0;
	i64 one = 1 << 30;

	while (one > op) {
		one = one >> 2;
	}

	while (one != 0) {
		if (op >= (res + one)) {
			op = op - (res + one);
			res = res + (2 * one);
		}

		res = res >> 1;
		one = one >> 2;
	}

	return res;
}

i64 isPrime(i64 n) {
	if (((n & 1) == 0)) {
		if (n != 2) {
			return 0;
		}
	}

	i64 i = sqrt(n);
	while (i >= 3) {
		if ((n % i) == 0) {
			return 0;
		}
		i = i - 1;
	}

	return 1;
}

i64 main() {
	i64 n = 1;
	i64 primeSum = 0;

	while (n < 2000000) {
		n = n + 1;

		if (isPrime(n) == 1) {
			primeSum = primeSum + n;
		}
	}

	return (primeSum >> 0);
}

